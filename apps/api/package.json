{
	"name": "@preceptoria/server",
	"version": "0.3.0",
	"type": "module",
	"module": "src/index.ts",
	"types": "src/index.ts",
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./src/index.ts"
		}
	},
	"mikro-orm": {
		"useTsNode": true,
		"tsNode": true
	},
	"scripts": {
		"test": "bun test",
		"test:watch": "bun test --watch",
		"test:coverage": "bun test --coverage",
		"dev": "bun run --watch src/index.ts",
		"lint": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs",
		"lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs --fix",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"type-check": "tsc --noEmit",
		"db:studio": "drizzle-kit studio",
		"db:cache": "bunx --bun mikro-orm cache:generate --combined",
		"db:migration:create": "bunx --bun mikro-orm migration:create",
		"db:migration:up": "bunx --bun mikro-orm migration:up",
		"db:migration:down": "bunx --bun mikro-orm migration:down",
		"db:migration:list": "bunx --bun mikro-orm migration:list",
		"db:schema:update": "bunx --bun mikro-orm schema:update",
		"db:schema:drop": "bunx --bun mikro-orm schema:drop",
		"db:seed": "bunx --bun mikro-orm seeder:run",
		"db:seed:create": "bunx --bun mikro-orm seeder:create",
		"sync:google-sheets": "bun scripts/syncGoogleSheets.ts",
		"test:seed": "bun scripts/testSeeder.ts",
		"run:seed": "bun scripts/runSeeder.ts",
		"build": "tsc -p tsconfig.json"
	},
	"dependencies": {
		"@elysiajs/bearer": "^1.3.0",
		"@elysiajs/cors": "^1.3.3",
		"@elysiajs/cron": "^1.3.0",
		"@elysiajs/eden": "^1.3.2",
		"@elysiajs/opentelemetry": "^1.3.0",
		"@elysiajs/server-timing": "^1.3.0",
		"@elysiajs/swagger": "^1.3.1",
		"@googleapis/drive": "^13.0.1",
		"@googleapis/sheets": "^10.0.1",
		"@mikro-orm/cli": "^6.4.16",
		"@mikro-orm/core": "^6.4.16",
		"@mikro-orm/migrations": "^6.4.16",
		"@mikro-orm/postgresql": "^6.4.16",
		"@mikro-orm/reflection": "^6.4.16",
		"@mikro-orm/seeder": "^6.4.16",
		"@types/bun": "^1.2.18",
		"class-validator": "^0.14.2",
		"dotenv": "^17.1.0",
		"elysia": "^1.3.5",
		"googleapis": "^150.0.1",
		"jose": "^6.0.11",
		"lru-cache": "^11.1.0"
	},
	"devDependencies": {
		"@elysiajs/jwt": "^1.3.1",
		"@eslint/js": "^9.30.1",
		"@types/pg": "^8.15.4",
		"@types/uuid": "^10.0.0",
		"ajv": "^8.17.1",
		"bun-types": "^1.2.18",
		"drizzle-kit": "^0.31.4",
		"drizzle-orm": "^0.44.2",
		"eslint": "^9.30.1",
		"eslint-config-standard": "^17.1.0",
		"eslint-plugin-import": "^2.32.0",
		"eslint-plugin-n": "^17.21.0",
		"eslint-plugin-promise": "^7.2.1",
		"globals": "^16.3.0",
		"pg": "^8.16.3",
		"prettier": "3.6.2",
		"ts-node": "^10.9.2",
		"tsx": "^4.20.3",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.36.0",
		"uuid": "^11.1.0"
	}
}
